generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Device {
  deviceId   Int      @id @default(autoincrement())
  mac        String   @unique
  ip         String[]
  state      String
  name       String
  type       String?
  make       String?
  model      String?
  first_seen String
}
